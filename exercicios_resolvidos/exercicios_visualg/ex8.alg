algoritmo "ex8"

var
     current_year, year_birth, age: Inteiro
inicio
      Escreval("What year are we in?")
      Leia(current_year)
      Escreval("What year was I born?")
      Leia(year_birth)
      age <- current_year - year_birth
      Escreva("My age will be: ", age)
fimalgoritmo
